#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <set>
#include <algorithm>
using namespace std;


int main() {
    set <int> s;
    int Q; cin >> Q;
    for (int i=0; i<Q; i++){
        int a, b; cin >> a >> b;
        if (a==1){
            s.insert(b);
        }
        if (a==2){
            s.erase(b);
        }
        if (a==3){
            set<int>::iterator itr=s.find(b);
            if (itr == s.end()) cout<<"No\n";
            else cout<<"Yes\n";
        }
    } 
    return 0;
}
